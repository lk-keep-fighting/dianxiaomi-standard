        "Warranty": "1 Year",
        "Certification": "CE Marked"
      },
      "clothing": {
        "Material": "Cotton",
        "Care Instructions": "Machine washable",
        "Season": "All Season"
      }
    }
  },
  
  "configuration": {
    "priority_order": [
      "exact_match",
      "pattern_match", 
      "contextual_defaults",
      "category_based",
      "type_based"
